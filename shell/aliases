# Jekyll
alias jk='jekyll'
alias jksw='jk serve --watch'

# Git
alias gh='git push'
alias gl='git pull'
alias ga='git add'
alias gc='git commit'
alias gd='git diff'
alias gs='git status'

# Rust
## Cargo build
alias cbd='cargo build'
alias cbr='cargo build --release'
alias cb='cbd'
## Cargo run
alias crd='cargo run'
alias crr='cargo run --release'
alias cr='crd'
## Cargo update
alias cu='cargo update'
## Cargo test
alias ct='cargo test'
# Cargo doc
alias cdoc='cargo doc'

# apt
alias agi='sudo apt-get install'
alias agiy='sudo apt-get install -y'
alias agr='sudo apt-get remove'
alias ags='apt-cache search'
alias agd='sudo apt-get update'
alias agg='sudo apt-get upgrade'

